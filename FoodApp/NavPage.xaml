<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="FoodApp.NavPage">
    <ContentPage.Content>

        <ScrollView>
        <StackLayout>
            <Button x:Name="ProfilePageBtn" Text="Profile Page" Clicked="ProfilePageBtn_Clicked"/>

            <Button x:Name="UserLoginBtn" Text="Log in" Clicked="UserLoginBtn_Clicked"/>

            <Button x:Name="SignUpBtn" Text="Sign Up" Clicked="SignUpBtn_Clicked"/>
            <Button x:Name="SearchBtn" Text="Search (remove later)" Clicked="SearchBtn_Clicked"/>
            <Button x:Name="PlaceList" Text="Show Random Place" Clicked="Handle_Clicked"/>
            <Button x:Name="nextPage" Text="next page" Clicked="Handle_Clicked_1Async"/>
            <Button x:Name="PlacePage" Text="PlacePage" Clicked="PlacePage_Clicked"/>

            <Image x:Name="img" />

            <Button x:Name="Swipe" Text="Swipe" Clicked="Swipe_Clicked"/>


        </StackLayout>
        </ScrollView>
        
    </ContentPage.Content>
</ContentPage>
